Func_with_many_args(a; b; c; d; e){
return a + b + c + d + e;
}

Func_with_While(condition){
    a = 10;
    while(condition > a){
        a = a + 5;
    };
    return a + 4*8^2;
}

Fain(){
    skip;
}

Main () {
   v = 4;
   Fain();
   v = Func_with_many_args(1; 1; 1; 1; 1);
   v = v + Func_with_While(20);
   if (v == 4) {
        v = v * (-10);
   } else {
        v = v * 10;
   };
}
